                                            rdfs:range xsd:string ;
                                            rdfs:comment "The intended purpose of the AI system, as per the AI Act."@en ;
                                            rdfs:label "intended purpose"@en .


###  https://w3id.org/aidoc-ap#modality
<https://w3id.org/aidoc-ap#modality> rdf:type owl:DatatypeProperty ;
                                     rdfs:domain <https://w3id.org/aidoc-ap#AISystem> ;
                                     rdfs:range xsd:string ;
                                     rdfs:comment "The physical form or modality in which the AI system is provided (e.g., standalone software, API)."@en ;
                                     rdfs:label "modality"@en .


###  https://w3id.org/aidoc-ap#version
<https://w3id.org/aidoc-ap#version> rdf:type owl:DatatypeProperty ;
                                    rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
                                    rdfs:range xsd:string ;
                                    rdfs:comment "The version of the AI system or artifact."@en ;
                                    rdfs:label "version"@en .


