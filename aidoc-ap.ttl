                                                          rdfs:range <https://w3id.org/aidoc-ap#AISystemCapability> ;
                                                          rdfs:comment "Relates an AISystem to an AISystemCapability it is designed to exhibit or support" ;
                                                          rdfs:label "has capability"@en ;
