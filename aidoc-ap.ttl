#################################################################
#    Ontology Metadata 
#################################################################

<https://w3id.org/aidoc-ap#>
    rdf:type owl:Ontology ;
    dcterms:title "AIDOC-AP - AI Documentation Application Profile"@en ;
    dcterms:description "The AI Documentation Application Profile (AIDOC-AP) is designed to provide a structured, machine-readable model of an AI system and its entire lifecycle. It is built as a modular framework, rooted in established semantic web standards and enhanced with domain-specific concepts from the research material provided. The core of AIDOC-AP is a graph-based representation that captures the provenance of all AI artifacts, the activities that create and modify them, and the agents responsible for those activities."@en ;
    dcterms:creator [
        rdf:type foaf:Person ;
        foaf:name "Sebastian Neumaier" ;
        foaf:mbox <mailto:sebastian.neumaier@ustp.at> ;
        foaf:organization "University of Applied Sciences St. Pölten"
    ], [
        rdf:type foaf:Person ;
        foaf:name "Fabian Kovac" ;
        foaf:mbox <mailto:fabian.kovac@ustp.at> ;
        foaf:organization "University of Applied Sciences St. Pölten"
    ], [
        rdf:type foaf:Person ;
        foaf:name "Timea Pahi" ;
        foaf:mbox <mailto:timea.pahi@ustp.at> ;
        foaf:organization "University of Applied Sciences St. Pölten"
    ] ;
    dcterms:publisher [
        foaf:name "CERTAIN Project" ;
        foaf:homepage <https://certain-project.eu> 
    ] ;
    dcterms:issued "2025-11-11"^^xsd:date ;
    owl:versionInfo "1.0" ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    vann:preferredNamespacePrefix "aidoc" ;
    vann:preferredNamespaceUri "https://w3id.org/aidoc-ap#" ;
    schema:keywords "AI systems, ontology, AIDOC-AP, AI lifecycle, AI Act"@en ;
    schema:funding <https://cordis.europa.eu/project/id/101189650> ;
    dcterms:language "en" ;
    rdfs:label "AIDOC-AP"@en ;
    owl:versionIRI <https://w3id.org/aidoc-ap/1.0> .
 

